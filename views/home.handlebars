<section class="container">
  <h2 class="my-2 mx-auto px-2" style = "color:white;">Trending</h2>
  <div class="row justify-content-between p-2 pt-0 mb-4">
    {{#each mostCommented}}
      {{> image-card this}}
        {{else}}
        <h3 class="empty" style = "color:white;">Whoops no content! Add some posts.</h3>
    {{/each}}
  </div>

  <h2 class="my-2 mx-auto px-2" style = "color:white;">Most Liked</h2>
  <div class="row justify-content-between p-2 pt-0 mb-4">
    {{#each mostLiked}}
      {{> image-card this}}
        {{else}}
        <h3 class="empty" style = "color:white;">Whoops no content! Add some posts.</h3>
    {{/each}}
  </div>
</section>
<script src="/js/jquery.min.js"></script>
<script src="/js/create-comment.js"></script>
<script src="/js/add-like.js"></script>


